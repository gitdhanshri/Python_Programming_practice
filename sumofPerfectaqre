def SumOfSquares(n):
	l=list(range(1,n+1))
	print(l)
	count=0
	start=0
	print(len(l))
	end=len(l)//4
	print(l[end])
	for i in (range(n//2+1,0,-1)):
		if i*i<=n:
			end=i
			break
	print("End-->",end)
	for i in range(2,end+1):
		while(i**2<=n):
			print(i)
			n-=i**2
			count+=1
	print("Count--->",count,n)
		
		
SumOfSquares(20)
	
